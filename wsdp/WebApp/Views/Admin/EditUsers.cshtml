@model List<Model.DTO.UserDTO>
@{
    ViewBag.Title = "EditUsers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/EditUsers")

@Styles.Render("~/content/EditUsers_css")
@Styles.Render("~/Content/css")

<h2>Configuring users</h2>

<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Id</th>
            <th>User Name</th>
            <th>Password</th>
            <th>Email</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            @Html.Partial("UserPartialView",user)
        }
    </tbody>
</table>